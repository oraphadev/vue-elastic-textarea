{
  "name": "vue-elastic-textarea",
  "version": "1.0.5",
  "description": "Simple elastic textarea component for Vue.js",
  "main": "dist/vue-elastic-textarea.ssr.js",
  "browser": "dist/vue-elastic-textarea.esm.js",
  "module": "dist/vue-elastic-textarea.esm.js",
  "unpkg": "dist/vue-elastic-textarea.min.js",
  "files": [
    "dist/*",
    "src/**/*.vue"
  ],
  "scripts": {
    "serve": "vue-cli-service serve dev/serve.js",
    "build": "cross-env NODE_ENV=production rollup --config build/rollup.config.js",
    "build:ssr": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format cjs",
    "build:es": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format es",
    "build:unpkg": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format iife"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "@rollup/plugin-alias": "^2.2.0",
    "@rollup/plugin-commonjs": "^11.1.0",
    "@rollup/plugin-replace": "^2.3.2",
    "@vue/cli-plugin-babel": "^4.3.1",
    "@vue/cli-service": "^4.3.1",
    "cross-env": "^7.0.2",
    "minimist": "^1.2.5",
    "rollup": "^2.7.3",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-terser": "^5.3.0",
    "rollup-plugin-vue": "^5.1.6",
    "vue": "^2.6.11",
    "vue-template-compiler": "^2.6.11"
  },
  "license": "MIT",
  "author": "Raphael Lima <oraphadev@gmail.com>",
  "bugs": {
    "url": "https://github.com/oraphadev/vue-elastic-textarea/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/oraphadev/vue-elastic-textarea.git"
  },
  "peerDependencies": {
    "vue": "^2.6.11"
  },
  "engines": {
    "node": ">=10"
  },
  "keywords": [
    "vue",
    "vue-elastic-textarea",
    "textarea"
  ]
}
